{
  "name": "electron-react-redux-starter-kit",
  "productName": "Simple Notes",
  "version": "1.0.0",
  "description": "Simple Notes is a notetaking app developed by Ashley Bibizadeh",
  "main": "index.js",
  "author": "Ashley Bibizadeh",
  "license": "MIT",
  "build": {
    "appId": "com.example.simple-notes",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "build/static_assets/mac/icon.png"
    },
    "win": {
      "target": "nsis",
      "icon": "build/static_assets/win/icon.png"
    },
    "files": [
      "node_modules/",
      "build/",
      "static_assets/",
      "*"
    ]
  },
  "scripts": {
    "webpack:build": "webpack --config webpack.js",
    "webpack:watch": "webpack --config webpack.js --watch",
    "electron:dev": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mw",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "classnames": "2.2.6",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "electron": "2.0.8",
    "electron-builder": "20.28.3",
    "extract-loader": "2.0.1",
    "file-loader": "2.0.0",
    "node-sass": "4.9.3",
    "react-ckeditor-wrapper": "1.1.2",
    "react-confirm-alert": "2.0.5",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "toastr": "2.1.4",
    "webpack": "2.3.2"
  },
  "dependencies": {
    "electron-is-dev": "0.3.0",
    "nedb": "1.8.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.7",
    "react-router": "4.0.0",
    "react-router-dom": "4.0.0",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0"
  }
}
